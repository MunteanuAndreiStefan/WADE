{
  "name": "Fake News",
  "version": "1.0",
  "description": "Real time fake news detection !",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://*.twitter.com/*"],
      "js": ["background.js"]
    }
  ],
  "permissions": [
    "https://*/"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Fake News"
  }
}
